<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.main.MainController">
	<children>
		<TextField fx:id="savePathTextField" editable="false" layoutX="71.0" layoutY="351.0" prefHeight="36.0" prefWidth="297.0" />
		<Button fx:id="savePathChooseButton" layoutX="376.0" layoutY="351.0" mnemonicParsing="false" onAction="#showFileSaveDialog" text="浏览">
			<font>
				<Font size="18.0" />
			</font>
		</Button>
		<Button fx:id="startCompressButton" layoutY="470.0" mnemonicParsing="false" onAction="#startCompress" prefHeight="53.0" prefWidth="446.0" text="开始压缩">
			<font>
				<Font size="18.0" />
			</font>
		</Button>
		<CheckBox fx:id="overWriteCheckBox" layoutX="10.0" layoutY="403.0" mnemonicParsing="false" onAction="#overWriteAction" prefHeight="25.0" prefWidth="126.0" text="覆盖原文件">
			<font>
				<Font size="18.0" />
			</font>
		</CheckBox>
		<Label layoutX="14.0" layoutY="12.0" text="文件列表">
			<font>
				<Font size="18.0" />
			</font>
		</Label>
		<ListView fx:id="fileListView" layoutY="47.0" prefHeight="246.0" prefWidth="446.0" />
