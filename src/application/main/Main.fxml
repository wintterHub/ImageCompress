			<font>
				<Font size="18.0" />
			</font>
		</Button>
		<Button layoutX="350.0" layoutY="293.0" mnemonicParsing="false" onAction="#removeSelectedItem" text="删除选中">
			<font>
				<Font size="18.0" />
			</font>
		</Button>
		<Button layoutX="254.0" layoutY="293.0" mnemonicParsing="false" onAction="#clearList" text="清空列表">
			<font>
				<Font size="18.0" />
			</font>
		</Button>
		<Button layoutY="293.0" mnemonicParsing="false" onAction="#showFileChooseDialog" prefHeight="36.0" prefWidth="96.0" text="添加文件">
			<font>
				<Font size="18.0" />
			</font>
		</Button>
		<Label layoutX="10.0" layoutY="357.0" text="保存到">
			<font>
				<Font size="18.0" />
			</font>
		</Label>
		<ProgressBar fx:id="compressProgressBar" layoutY="448.0" prefHeight="23.0" prefWidth="446.0" progress="0.0" />
      <TextField fx:id="scaleTextField" layoutX="189.0" layoutY="401.0" prefHeight="32.0" prefWidth="80.0" promptText="默认100" />
      <TextField fx:id="qualityTextField" layoutX="337.0" layoutY="401.0" prefHeight="32.0" prefWidth="80.0" promptText="默认30" />
      <Label layoutX="150.0" layoutY="405.0" prefHeight="24.0" prefWidth="37.0" text="缩放">
         <font>
            <Font size="18.0" />
