<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="340.0" prefWidth="278.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.main.MainController">
	<children>
						<TextField fx:id="savePathTextField" editable="false" layoutX="65.0" layoutY="351.0" prefHeight="36.0" prefWidth="149.0" />
						<Button fx:id="savePathChooseButton" layoutX="214.0" layoutY="351.0" mnemonicParsing="false" onAction="#showFileSaveDialog" text="浏览">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
						<Button fx:id="startCompressButton" layoutY="428.0" mnemonicParsing="false" onAction="#startCompress" prefHeight="53.0" prefWidth="414.0" text="开始压缩">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <CheckBox fx:id="overWriteCheckBox" layoutX="282.0" layoutY="356.0" mnemonicParsing="false" onAction="#overWriteAction" prefHeight="25.0" prefWidth="126.0" text="覆盖原文件">
         <font>
            <Font size="18.0" />
         </font>
      </CheckBox>
      <Label layoutX="14.0" layoutY="12.0" text="文件列表">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <ListView fx:id="fileListView" layoutY="47.0" prefHeight="246.0" prefWidth="414.0" />
						<Button layoutX="96.0" layoutY="293.0" mnemonicParsing="false" onAction="#showDirectoryChooseDialog" text="添加文件夹">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
						<Button layoutX="318.0" layoutY="293.0" mnemonicParsing="false" onAction="#removeSelectedItem" text="删除选中">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
						<Button layoutX="222.0" layoutY="293.0" mnemonicParsing="false" onAction="#clearList" text="清空列表">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
						<Button layoutY="293.0" mnemonicParsing="false" onAction="#showFileChooseDialog" prefHeight="36.0" prefWidth="96.0" text="添加文件">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Label layoutX="5.0" layoutY="357.0" text="保存到">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <ProgressBar fx:id="compressProgressBar" layoutY="406.0" prefHeight="23.0" prefWidth="414.0" progress="0.0" />
      <Button layoutX="346.0" layoutY="6.0" mnemonicParsing="false" onAction="#advancedConfig" prefHeight="36.0" prefWidth="60.0" text="高级">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
	</children>
</AnchorPane>
